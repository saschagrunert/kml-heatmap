{
  "name": "kml-heatmap",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "npx prettier --write .",
    "format:check": "npx prettier --check .",
    "build": "node build.js",
    "build:dev": "NODE_ENV=development node build.js",
    "build:watch": "node build.js --watch",
    "update-deps": "ncu -u"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@types/leaflet": "^1.9.21",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/ui": "^4.0.18",
    "esbuild": "^0.27.3",
    "eslint": "^10.0.1",
    "jsdom": "^28.1.0",
    "npm-check-updates": "^19.4.1",
    "prettier": "^3.8.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.56.0",
    "vitest": "^4.0.18"
  },
  "dependencies": {
    "dompurify": "^3.3.1"
  },
  "overrides": {
    "typescript-eslint": {
      "eslint": "$eslint"
    }
  }
}
